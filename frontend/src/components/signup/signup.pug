#signup
    //- ユーザーのSingUpフォーム
    .inner
       .container(style="margin-top: 20px")
            .col-sm-6.col-sm-offset-3
                h3 SignUp

                alert(
                    :show.sync="alertProp.show",
                    :type="alertProp.type",
                    dismissable
                )
                    span {{alertProp.message}}

                form
                    bs-input(
                        required icon,
                        type="email",
                        label="email",
                        :value.sync="user.email",
                        error="Insert your email"
                    )
                    bs-input(
                        required icon,
                        label="password",
                        type="password",
                        :value.sync="user.password",
                        error="Insert password",
                        minLength="6"
                    )
                    bs-input(
                        required icon,
                        label="password-confilm",
                        type="password",
                        :value.sync="passwordConfilm",
                        error="Don't match password",
                        :match="user.password"
                    )

                    button.form-control.btn.btn-primary(
                        type="button",
                        :disabled="!formValidate()",
                        @click="signUp()"
                    ) SignUp


